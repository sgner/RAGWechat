    "t-loading": "./miniprogram/miniprogram_npm/tdesign-miniprogram/loading/loading",
    "t-fab": "./miniprogram/miniprogram_npm/tdesign-miniprogram/fab/fab",
    "t-empty": "./miniprogram/miniprogram_npm/tdesign-miniprogram/empty/empty",
    "t-popup": "./miniprogram/miniprogram_npm/tdesign-miniprogram/popup/popup",
    "t-dialog": "./miniprogram/miniprogram_npm/tdesign-miniprogram/dialog/dialog",
    "t-swiper": "./miniprogram/miniprogram_npm/tdesign-miniprogram/swiper/swiper",
