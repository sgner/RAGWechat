      {
        "pagePath": "pages/chat/chat",
        "text": "对话",
        "iconPath": "assets/icons/chat.png",
        "selectedIconPath": "assets/icons/chat-active.png"
      },
      {
        "pagePath": "pages/profile/profile",
        "text": "我的",
        "iconPath": "assets/icons/profile.png",
        "selectedIconPath": "assets/icons/profile-active.png"
      }
    ]
  },
  "usingComponents": {
    "t-button": "./miniprogram/miniprogram_npm/tdesign-miniprogram/button/button",
    "t-icon": "./miniprogram/miniprogram_npm/tdesign-miniprogram/icon/icon",
    "t-avatar": "./miniprogram/miniprogram_npm/tdesign-miniprogram/avatar/avatar",
